{
  "module": {
    "name": "entry",
    "type": "entry",
    "description": "$string:module_desc",
    "mainElement": "EntryAbility",
    "deviceTypes": [
      "phone",
      "tablet",
      "2in1"
    ],
    "deliveryWithInstall": true,
    "installationFree": false,
    "pages": "$profile:main_pages",
    "abilities": [
      {
        "name": "EntryAbility",
        "srcEntry": "./ets/entryability/EntryAbility.ets",
        "description": "$string:EntryAbility_desc",
        "icon": "$media:launcher",
        "label": "$string:EntryAbility_label",
        "startWindowIcon": "$media:launcher",
        "startWindowBackground": "$color:start_window_background",
        "exported": true,
        "skills": [
          {
            "entities": [
              "entity.system.home",
              "entity.system.browsable"
            ],
            "actions": [
              "action.system.home",
              "ohos.want.action.viewData"
            ],
            "uris": [
              {
                "scheme": "https",
                "host": "sharekitdemo.drcn.agconnect.link"
              },
              {
                "scheme": "file",
                "type": "com.example.sharekit_sample_demo.share",
                "maxFileSupported": 1
              },
              {
                "scheme": "file",
                "linkFeature": "FileOpen",
                "type": "org.openxmlformats.wordprocessingml.document",
                "maxFileSupported": 1
              }
            ],
            "domainVerify": true
          }
        ]
      },
      {
        "name": "ShareUIAbility",
        "srcEntry": "./ets/entryability/ShareUIAbility.ets",
        "description": "$string:EntryAbility_desc",
        "icon": "$media:launcher",
        "label": "$string:ShareUIAbility_label",
        "startWindowIcon": "$media:launcher",
        "startWindowBackground": "$color:start_window_background",
        "exported": true,
        "skills": [
          {
            "actions": [
              "ohos.want.action.sendData"
            ],
            "uris": [
              {
                "scheme": "file",
                "utd": "general.text",
                "maxFileSupported": 1
              }
            ]
          }
        ]
      },
      {
        "name": "SubShareUIAbility",
        "srcEntry": "./ets/entryability/SubShareUIAbility.ets",
        "description": "$string:EntryAbility_desc",
        "icon": "$media:launcher",
        "label": "$string:SubShareUIAbility_label",
        "startWindowIcon": "$media:launcher",
        "startWindowBackground": "$color:start_window_background",
        "exported": true,
        "skills": [
          {
            "actions": [
              "ohos.want.action.sendData"
            ],
            "uris": [
              {
                "scheme": "file",
                "utd": "general.png",
                "maxFileSupported": 1
              }
            ]
          }
        ]
      }
    ],
    "extensionAbilities": [
      {
        "name": "ShareExtensionAbility",
        "srcEntry": "./ets/extensionAbility/ShareExtAbility.ets",
        "description": "$string:EntryAbility_desc",
        "icon": "$media:launcher",
        "label": "$string:ShareExtensionAbility_label",
        "type": "share",
        "exported": true,
        "skills": [
          {
            "actions": [
              "ohos.want.action.sendData"
            ],
            "uris": [
              {
                "scheme": "file",
                "utd": "general.jpeg",
                "maxFileSupported": 1
              }
            ]
          }
        ]
      },
      {
        "name": "SubShareExtensionAbility",
        "srcEntry": "./ets/extensionAbility/SubShareExtAbility.ets",
        "description": "$string:EntryAbility_desc",
        "icon": "$media:launcher",
        "label": "$string:SubShareExtensionAbility_label",
        "type": "share",
        "exported": true,
        "skills": [
          {
            "actions": [
              "ohos.want.action.sendData"
            ],
            "uris": [
              {
                "scheme": "file",
                "utd": "general.png",
                "maxFileSupported": 1
              }
            ]
          }
        ]
      }
    ],
    "metadata": [
      {
        "name": "shareType",
        "value": "sharekitModel",
      },
      {
        "name": "shareBundleName",
        "value": "com.example.sharekit_sample_demo",
      }
    ],
    // 需申请该权限后可使用（该权限仅支持企业应用申请） 可配置目标应用名单
//    "requestPermissions": [
//      {
//        "name": "ohos.permission.SET_SYSTEMSHARE_APPLAUNCHTRUSTLIST",
//        "reason": '$string:permission_reason_set_system_share_app_launch_trust_list'
//      },
//    ]
  }
}