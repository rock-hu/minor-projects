{
  "supportFileExts": [
    ".ets",
    ".ts",
    ".js",
    ".d.ets",
    ".d.ts"
  ],
  "enableLeadingComments": false,
  "enableBuiltIn": true,
  "ignoreFileNames": [
    "oh_modules",
    "node_modules",
    "build-tools",
    "hvigorfile.ts",
    "hvigorfile.js",
    "hvigor-wrapper.js"
  ],
  "sdkGlobalFolders": [
    "component",
    "@internal",
    "ohos-typescript",
    "dist"
  ],
  "tsconfig": "tsconfig.json"
}
