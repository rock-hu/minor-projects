    "entryPoints": [
        "./src/index.ts"
    ],
    "excludeInternal": true,
    "useTsLinkResolution": true,
    "plugin": "typedoc-plugin-markdown",
    "out": "docs/api_docs",
    "readme": "./README.en.md"
}