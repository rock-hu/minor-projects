class B {

}

class C extends B {
    a: int = 12;
}


class A<T> {
    foo(klazz: Class<T>) : T {
        return klazz.newInstance();
    }
}

function main(): void {
    let a = new A<C>;

    let c = a.foo(C.class);
    assert c.a == 12;
}
