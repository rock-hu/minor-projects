function main() {
    let x : Record<number, string> = {
        1:"hello",
        2:"hello2",
        3:"world",
    };
    assertEQ(x[1], "hello")
    assertEQ(x[2], "hello2")
    assertEQ(x[3], "world")

    x[1] = "newHello";
    assertEQ(x[1], "newHello")
} 