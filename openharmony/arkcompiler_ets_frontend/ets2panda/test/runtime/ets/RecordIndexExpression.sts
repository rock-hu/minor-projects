function main() {
    let x : Record<number, string> = {
        1:"hello",
        2:"hello2",
        3:"world",
    };
    assert x[1] == "hello";
    assert x[2] == "hello2";
    assert x[3] == "world";

    x[1] = "newHello";
    assert x[1] == "newHello";
} 