function main() {
    let c: char = c'a'
    let i: Int = c
    let l: Long = c
    let f: Float = c
    let d: Double = c
    assertEQ(i.unboxed(), 97)
    assertEQ(l.unboxed(), 97)
    assertEQ(f.unboxed(), 97.0)
    assertEQ(d.unboxed(), 97.0)
}

