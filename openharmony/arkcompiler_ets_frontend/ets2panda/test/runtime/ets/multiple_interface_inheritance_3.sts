let res: number = 0;

interface I {
    foo(i: number) {
        return 1;
    }
}
interface J extends I {
    foo(j: string) {
        return 2;
    }
}

class B {
    foo(j: string) {
        return 3;
    }

}
class A extends B implements J, I {
    foo(i: number) {
        return 4;
    }
}

function main(): void {
    let a = new A();
    assertEQ(a.foo(1.1 as number), 4);
    assertEQ(a.foo("some string"), 3);
}