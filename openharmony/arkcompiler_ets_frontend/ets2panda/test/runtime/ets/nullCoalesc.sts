let b : Int = 10;

function foo(): Int {
  b = 11;
  return 12;
}

function main() : void {
  let a : Int = 1;
  let c = a ?? foo();
  assert(b == 10);
  assert(c == 1);

  a = null;
  c = a ?? b;
  assert (c == 10);

  c = a ?? foo();
  assert (b == 11);
  assert (c == 12);

  let d : Boolean = true;
  let e : boolean = false;
  let f = d ?? e;
  assert (f == true);
}
