class A {
    readonly fld: number = 2;
    fld2: number = 3;
}
function main(): void {
    let a: A = {fld: 3, fld2: 4};
    a.fld2 = 5;
    assert(a.fld == 3);
    assert(a.fld2 == 5);
}
