interface IButton {
  getTitle(): string;
  setTitle(t: string): IButton;
}

class Button implements IButton {
  title: string = "";

  getTitle(): string {
    return this.title;
  }

  setTitle(t: string): this {
    this.title = t;
    return this;
  }
}

function main() {
  let b = new Button().setTitle("üçÜ");

  assertTrue(b instanceof Button)
  assertEQ(b.getTitle(), "üçÜ")
}
