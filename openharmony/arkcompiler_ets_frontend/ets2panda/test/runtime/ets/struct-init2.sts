struct cls {
  readonly const_member_int : int = 10;
  member_int : int = 20;
  static readonly const_static_member_int : int = 30;
  static static_member_int : int = 40;
}

function main(): void {
    let a : cls = new cls();
    assert (a.const_member_int == 10)
    assert (a.member_int == 20)
    assert (cls.const_static_member_int == 30)
    assert (cls.static_member_int == 40)
    return;
}
