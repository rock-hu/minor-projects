class Component {
  width: int;

  getWidth(): int {
    return this.width;
  }

  setWidth(w: int): this {
    this.width = w;

    return this;
  }
}

class Button extends Component {
  title: string = "";

  getTitle(): string {
    return this.title;
  }

  setTitle(t: string): this {
    this.title = t;

    return this;
  }
}

function main() {
  let b = new Button().setWidth(50).setTitle("üçÜ");

  assert b instanceof Button;
  assert b.getWidth() == 50;
  assert b.getTitle() == "üçÜ";
}
