import("//arkcompiler/ets_runtime/js_runtime_config.gni")
import("$build_root/test.gni")

declare_args() {
  TEST_LLVM_ONLY = false
}

if (is_standard_system || ark_standalone_build) {
  _icu_path_ = "thirdparty/icu"
} else {
  _icu_path_ = "global/i18n"
