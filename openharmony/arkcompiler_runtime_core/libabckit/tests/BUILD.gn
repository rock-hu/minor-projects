template("gen_merge_abc") {
  merge_datas = invoker.merge_datas

  merge_file_info = merge_datas[0]
  anno_test_abc =
      "$target_out_dir/${merge_file_info[0]}/${merge_file_info[1]}.abc"
  anno_merge_file =
      "$target_out_dir/${merge_file_info[0]}/${merge_file_info[1]}_merge.txt"
  anno_path_prefix = rebase_path("$test_js_path")
  anno_test_merge_path = rebase_path(anno_merge_file)
  content = ""
  foreach(data, merge_datas) {
    if (defined(invoker.is_ohmurl) && invoker.is_ohmurl == true) {
      item = "$anno_path_prefix/${data[0]}/${data[1]}.ts;&${data[1]}&;esm;${data[1]}.ts;entry;false;ets"
    } else {
      item = "$anno_path_prefix/${data[0]}/${data[1]}.ts;${data[1]};esm;${data[1]}.ts;entry;false;ets"
    }
    if (content == "") {
      content = "${content}echo '$item' > $anno_test_merge_path ; "
    } else {
      content = "${content}echo '$item' >> $anno_test_merge_path ; "
    }
  }
  action("${target_name}_gen_anno_merge_file") {
    script = "/usr/bin/sh"
    args = [
      "-c",
      "$content",
    ]
    outputs = [ anno_merge_file ]
  }

  es2abc_gen_abc("${target_name}") {
    extra_visibility = [ ":*" ]
    extra_dependencies = [ ":${target_name}_gen_anno_merge_file" ]
    src_js = "@" + anno_test_merge_path
    dst_file = rebase_path(anno_test_abc)
    extra_args = [
      "--module",
      "--merge-abc",
      "--enable-annotations",
    ]
    extension = "ts"
    in_puts = [ anno_merge_file ]
    out_puts = [ anno_test_abc ]
  }
}

