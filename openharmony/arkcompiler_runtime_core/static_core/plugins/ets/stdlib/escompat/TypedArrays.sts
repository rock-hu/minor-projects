        if (compareFn != undefined) {
            cmp = (l: float, r: float): number => {
                return compareFn!(l as number, r as number)
            }
        }
        sort(arr, cmp)
        for (let i = 0; i < this.lengthInt; ++i) {
            this.setUnsafe(i, arr[i])
        }
        return this
    }

