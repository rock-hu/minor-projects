        if (compareFn != undefined) {
            cmp = (l: long, r: long): number => {
                return compareFn!(new BigInt(l), new BigInt(r))
            }
        }
        sort(arr, cmp)
        for (let i = 0; i < this.lengthInt; ++i) {
            this.setUnsafe(i, arr[i])
        }
        return this
    }

