        }
        let k: int = this.lengthInt + fromIndex
        if (fromIndex >= 0) {
            k = (this.lengthInt - 1 < fromIndex) ? this.lengthInt - 1 : fromIndex
        }
        while (k >= 0) {
            if (this.getUnsafe(k) == searchElement) {
                return k
            }
            k--
        }
        return -1
    }

