/**
 * Copying version of the splice() method.
 *
 * @param start index
 *
 * @param delete number of items after start index
 *
 * @returns a new Array with some elements removed and/or replaced at a given index.
 */
export function toSpliced(self: FixedArray<char>, start: int, delete: int, ...items: FixedArray<char>): FixedArray<char> {
    const len = self.length;
    start = normalizeIndex(start, len);
    if (delete < 0) {
        delete = 0;
    } else if (delete > len) {
        delete = len;
    }
    if (start > len - delete) {
        delete = len - start
    }
    const res : FixedArray<char> = new char[len - delete + items.length];
    for (let i = 0; i < start; i++) {
        res[i] = self[i]
    }
    for (let i = 0; i < items.length; i++) {
        res[start + i] = items[i]
    }
    for (let i = start + delete; i < len; i++) {
        res[i - delete + items.length] = self[i]
    }
    return res;
}

