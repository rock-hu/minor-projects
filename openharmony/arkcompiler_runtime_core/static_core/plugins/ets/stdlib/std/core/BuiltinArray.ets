export function toSpliced(self: FixedArray<short>, start: int, delete: int, ...items: FixedArray<short>): FixedArray<short> {
    const len = self.length;
    start = normalizeIndex(start, len);
    if (delete < 0) {
        delete = 0;
    } else if (delete > len) {
        delete = len;
