 */
export function join(self: FixedArray<long>, sep?: String): string {
    if (self.length == 0) {
        return ""
    }
    const sepReal = sep === undefined ? "," : sep!
    let sb = new StringBuilder(new String(self[0]))
    for (let i: int = 1; i < self.length; i++) {
        const tmp = self[i]
        sb.append(sepReal);
        sb.append(tmp)
    }

    return sb.toString();
}

