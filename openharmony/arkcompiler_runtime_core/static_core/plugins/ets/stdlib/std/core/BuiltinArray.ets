 */
export function reduceRight<U>(self: FixedArray<char>, callbackfn: (previousValue: U, currentValue: char, index: number, array: FixedArray<char>) => U, initialValue: U): U {
    let acc = initialValue
    for (let i = self.length - 1; i >= 0; i--) {
