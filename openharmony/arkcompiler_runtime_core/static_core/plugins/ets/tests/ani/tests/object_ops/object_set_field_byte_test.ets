class Pack {
    boolean_value: boolean = true;
    string_value: string = "";
    byte_value: byte = 0;
}

function newPackObject(): Pack {
    return new Pack();
}

function checkByteValue(p: Pack, value: byte): boolean {
    return p.byte_value === value;
}
