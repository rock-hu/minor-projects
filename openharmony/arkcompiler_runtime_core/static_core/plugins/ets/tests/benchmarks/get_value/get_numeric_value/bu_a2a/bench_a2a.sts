import {
    stsNumber,
    stsByte,
    stsShort,
    stsInt,
    stsLong,
    stsFloat,
    stsDouble
} from './test_import';

export class A2a {
    state: number = 0
    totalTime: number = 0;
    runsLeft: number = 0;

    public setup(): void {
        console.log('Starting')
    }

    test(): number {
        while (this.runsLeft > 0 ) {
            const start = Chrono.nanoNow();

            const numVal = stsNumber;
            const byteVal = stsByte;
            const shortVal = stsShort;
            const intVal = stsInt;
            const longVal = stsLong;
            const floatVal = stsFloat;
            const doubleVal = stsDouble;

            const end = Chrono.nanoNow();

            this.runsLeft = this.runsLeft - 1;
            this.totalTime = this.totalTime + (end - start);
        }
        return this.totalTime;
    }
}
