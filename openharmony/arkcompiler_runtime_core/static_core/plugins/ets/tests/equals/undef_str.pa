.language eTS

.record std.core.Object <external>
.record std.core.String <external>

.function i32 main() <static> {
    ets.ldundefined
    sta.obj v0
    lda.str "Hello, world!"
    sta.obj v1
    movi v2, 0x0
    ets.equals v0, v1
    jne v2, error
    ldai 0x0
    return
error:
    ldai 0x1
    return
}
