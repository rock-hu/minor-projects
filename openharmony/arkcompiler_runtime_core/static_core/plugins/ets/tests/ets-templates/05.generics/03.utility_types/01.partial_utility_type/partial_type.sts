{% for prop in properties %}
{% for c in cases %}

/*---
desc: Type Partial<T> constructs a type with all properties of T set to optional. T must be a class or an interface type.
---*/

class A {
  {{prop.i}} = 0
  {{prop.j}} = ""
}

function foo(i: Partial<A>) {
  let v: int = 0;
  if(i.j != undefined) {
    v += 1;
  }
  if(i.i != 0) {
    v += 2;
  }
  return v;
}

function main() {
  assert( foo( {{c.argument}} ) == {{c.result}} )
}

