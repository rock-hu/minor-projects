{% for c in cases %}

/*---
desc:  No method (including any getter or setter) of T is part of the Required<T> type.
---*/

let z: int = 0;

interface I {
  i?: number;
  j?: string;
}

class C implements I {
  set i(i: number) { z += 1; this.i = i; }
  get i(): number|undefined { z += 10; return this.i; }
  set j(j: string) { z += 5; this.j = j; }
  get j(): string|undefined { z += 50; return this.j; }
}

function fooI(i: Required<I>) {
  let v: int = 0;
  if(i.j != undefined) {
    v += 1;
  }
  if(i.i != 0) {
    v += 2;
  }
  return v;
}

function fooC(i: Required<I>) {
  let v: int = 0;
  if(i.j != undefined) {
    v += 1;
  }
  if(i.i != 0) {
    v += 2;
  }
  return v;
}

function main() {
  z = 0;
  assert( fooC( {{c.argument}} ) == {{c.result}} )
  assert( z == 0 );
  assert( fooI( {{c.argument}} ) == {{c.result}} )
  assert( z == 0 );
}

{% endfor %}
