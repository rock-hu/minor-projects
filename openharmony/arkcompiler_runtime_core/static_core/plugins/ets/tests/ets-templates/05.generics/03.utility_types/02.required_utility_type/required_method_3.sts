/*---
desc: >-
    Using interface with method as T for Required<T>.
    Method is implemented during the initialization.
tags: [compile-only, negative]
---*/

interface I {
  i: number = 0
  foo(): void
}

function foo(i: Required<I>) {
  let v: int = 0;
  if(i.i != 0) {
    v += 1;
  }
  return v;
}

function main() {
  assert( foo( {i: 1.9, foo() {}} ) == 1 )
}
