{%- for c in cases %}

/*---
desc: Call context, constructor, constant narrowing integer conversion, implicit
tags: [compile-only, negative]
---*/
class A {
    public f: {{c.to_type}}
    constructor(p: {{c.to_type}}) { this.f = p }
}

function foo(): {{c.from_type}} { return {{c.expr|safe}} }

function main(): int {
    // expression is not a constant expression
    if (new A(foo()).f == {{c.val}}) {
        return 1;
    }
    return 0;
}
{%- endfor %}
