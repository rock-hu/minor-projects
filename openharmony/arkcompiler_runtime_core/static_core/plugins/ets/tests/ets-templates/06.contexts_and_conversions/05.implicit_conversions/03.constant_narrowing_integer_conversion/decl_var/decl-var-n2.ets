{%- for c in cases %}
/*---
desc: Declaration context, variable declaration, constant narrowing integer conversion, implicit
tags: [compile-only, negative]
---*/
let v: {{c.from_type}} = {{c.expr|safe}}

function foo(): {{c.from_type}} {
    return v
}

function main(): int {
    let d: {{c.to_type}} = foo()  // expression isn't a constant expression
    if (d == {{c.val}}) {
        return 1;
    }
    return 0;
}
{%- endfor %}
