{%- for c in cases %}

/*---
desc: Call context, method, literal type to type of literal conversion, implicit
params: to {{c.to_type}}
tags: [{{c.tags}}]
---*/

let err1 = 1;
let err2 = 2;

{%- for v in c['values'] %}
class A{{loop.index}} {
    meth(p: {{v.type}}): {{c.to_type}} {
      if (err1 >= err2) {
        throw new Error()
      } else {
        return p
      }
    }
}
{%- endfor %}

function main() {
{%- for v in c['values'] %}
    assert(new A{{loop.index}}().meth({{v.expr}}) == {{v.val or v.expr}})
{%- endfor %}
}
{%- endfor %}
