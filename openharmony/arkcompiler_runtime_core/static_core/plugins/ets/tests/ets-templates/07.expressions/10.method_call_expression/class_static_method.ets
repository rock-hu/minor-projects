/*---
desc: Call instance method of a class.
assert: A method call expression calls a static or instance method of a class or an interface.
---*/

class A {
    public static foo(x: int): int {
        return x * 2;
    }
}

function main(): void {
    assertTrue( A.foo(3) == 6 )
}
