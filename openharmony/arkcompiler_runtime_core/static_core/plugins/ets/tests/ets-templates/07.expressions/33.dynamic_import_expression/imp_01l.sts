/*---
desc: Dynamic import expression allows loading a compilation unit asynchronously and dynamically.
tags: [negative, compile-only]
---*/

const path = "./wrong_dir" + "/" + "mod_01"

function main() {
    let promise: Promise<DynamicObject> = import(path)  // wrong constant expression path, CTE expected
    let dyn_obj = await promise
    assert(dyn_obj != null)
}
