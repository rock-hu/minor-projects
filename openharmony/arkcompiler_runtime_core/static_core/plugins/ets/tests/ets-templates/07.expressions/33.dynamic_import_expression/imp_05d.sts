/*---
desc: Dynamic import expression allows loading a compilation unit asynchronously and dynamically.
files: ["./aux_05/mod_05.sts"]
---*/

let dir = "./aux_05"
let file = "mod_05"

function main() {
    // import interface
    import(dir + "/" + file)  // dynamic path
    .then((dyn_obj) => {
        let c1: dyn_obj.Int1 = { name: "John", age: 42 }
        assert(c1.name == "John")
        assert(c1.age == 42)
    })
}
