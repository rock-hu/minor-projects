/*---
desc: Implementing getter from interface
---*/

interface I {
    get text(): string
    set text(s: string): void
}

class B implements I {
    private _text : string = "";

    get text(): string {
        return this._text;
    }

    set text(s: string): void {
        this._text = s;
    }
}

function main(): void {
    let instance: B = new B();

    instance.text = "text";
    assert instance.text == "text";
}
