/*---
desc: Validate that the child class cannot modify the readonly property of the parent class.
tags: [compile-only, negative]
---*/

interface G {
    attemptModify() {
        return "attemptModify from interface G";
    }
}

interface F extends G {

}

class A {
    readonly value: number = 1;
}

class B extends A implements F {
    attemptModify() {
        this.value = 2;
    }
}

function main(): void {
    const a: A = new A();
}