/*---
desc: Method signature compatibility with the same name.
---*/

let result: string = "";

interface Parent1 {
  greet(name: string): void;
}

interface Parent2 {
  greet(name: string): void;
}

class MyClass implements Parent1, Parent2 {
  greet(name: string): void {
    result = `Hello, ${name}!`
  }
}

function main(): void {
  const obj = new MyClass();
  obj.greet("Alice");
  assertEQ(result, "Hello, Alice!");
}