/*---
desc: >-
    ArkTS provides extended semantics for conditional-and and conditional-or expressions for better alignment.
---*/

{{c.decl}}

let retcode: int

function main(): int {
    let v: {{c.type}} = {{c.val}}
    retcode = 0

    while (v) {  // falsy
        if (retcode == 0) {
            retcode = 1
            break
        }
    }
    return retcode
}
