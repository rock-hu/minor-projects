/*---
desc: Test cases for various usage scenarios of asynchronous functions.
      Asynchronous function returns object.
---*/

class Person {
  name: string = '';
  age: number = 0;
  constructor(name: string, age: number) {
    this.name = name;
    this.age = age;
  }
}

async function functionReturnObject(): Promise<Person> {
  return new Person("Alice", 25);
}

async function startTest() {
  const result = await functionReturnObject();
  assertEQ(result.name === "Alice", true);
  assertEQ(result.age === 25, true);
}

function main(): void {
  startTest();
}