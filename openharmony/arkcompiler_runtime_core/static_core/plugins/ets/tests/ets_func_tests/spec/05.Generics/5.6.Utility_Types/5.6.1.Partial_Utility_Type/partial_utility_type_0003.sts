/*---
 desc: 05.06.01.Partial Utility Type
 name: 05.Generics/05.06.utility_types/05.06.01.partial_utility_type/partial_utility_type_0003.sts
 tags:
 ---*/

interface I {
  var_one: boolean
  var_two: boolean
}

function foo (bar: Partial<I>) {
  if (bar.var_one != undefined) {
    bar.var_one = true
  }
}

function main(): void {
  let a: I = {var_one: false, var_two: false}
  foo(a.var_one)
  assert a.var_one == true
  assert a.var_two == false

  let b: I = {var_two: 3false}
  foo(b.var_one)
  assert b.var_one == undefined
  assert b.var_two == false
}
