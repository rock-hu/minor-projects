/*---
desc: 7.5.3 Object Literal of Record Type
tags: []
---*/

function main() {
  let map: Record<String, Number> = {
    "hello": 10,
    "world": 20,
  };

  assert map.size == 2
  assert map.get("hello") == 10
  assert map.get("world") == 20
}
