/*---
desc: 9.6 Method_Declaration
tags: [compile-only]
---*/


interface I {
    static foo() : int  {return 0};
    foo() : int;
}

interface I1 extends I {
    static foo() : int {return 1};
}


class X implements I, I1 {
    foo(): int  {return 2}
}


function main() : int {
    let x : X = new X();
    assert x.foo() == 2;
    assert I1.foo() == 1;
    assert I.foo() == 0
    return 0;
}
