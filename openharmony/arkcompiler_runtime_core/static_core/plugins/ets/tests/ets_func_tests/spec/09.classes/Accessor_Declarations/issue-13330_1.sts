tags: []
---*/

let accessCounter = 0;

class Base {
    get base(): int {
        accessCounter++;
        return 41;
    }
}

class Derive extends Base {

    get derive(): int {
        return this.base + 1;
    }
}

function main(): void {
    accessCounter = 0;
    let vv = new Derive();

    assert vv.base == 41;
    assert accessCounter == 1;
    assert vv.derive == 42;
    assert accessCounter == 2;



}
