 name: issue-15142_4.sts
 tags: []
 ---*/

 interface TestGet {
    get value(): int
 }

 interface TestSet {
    set value(value: int);
 }

 interface Common extends TestGet, TestSet {
 }


 class Provider implements Common {
    value: int = 3;
 }

 function main(): void {
    let test = new Provider();
    assert test.value == 3;
    test.value = 4;
    assert test.value == 4;
 }
