/*---
desc: 16.6 Callable Types
name: type-call-expression-with-param_2.ets


---*/

let counter: int = 0;

class CallMeTrinity {

    static $_invoke(...assign: int[]) { counter += assign.length; }
}

function main(): int {
    counter = 0;
    CallMeTrinity.$_invoke(1,1,1);
    return counter == 3 ? 0 : 1;
}
