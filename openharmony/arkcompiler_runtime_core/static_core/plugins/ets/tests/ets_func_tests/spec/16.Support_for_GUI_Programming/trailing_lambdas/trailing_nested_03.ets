/*---
desc: >-
    17.10 Trailing lambda
tags: []
---*/

function fa(fn: () => int): int {
    return fn()
}

function nested(fn: () => int): void {
    assertEQ( fn(), 15)
}

function main(): void {
    nested() {
        return 3 * fa() {
            return 5
        }
    }
}

