/*---
 desc: 17.3 Indexable Types
 tags:
 ---*/

class Bar {
    private data : int []
    $_get (index: int) : int {return this.data[index]}
    $_set (index: int, value: int)  {this.data[index] = value}

    constructor(data : int[]) {
        this.data = data;
    }
}

function main() {
    let x : Bar = new Bar([1,2,3]);
    try {
       assertEQ(x[5], 1)
    } catch (e) {
        return 0;
    }

    return 1;
}
