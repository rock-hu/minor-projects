import { Decimal } from "@arkts.math.Decimal";

function checkToDecimalPlacesResult(expected: string, n: number | string, d?: number, r?: number) {
    if (d == undefined && r == undefined) {
        arktest.assertEQ(new Decimal(n).toDecimalPlaces().valueOf(), expected);
        return;
    }
    if (d != undefined && r == undefined) {
        arktest.assertEQ(new Decimal(n).toDecimalPlaces(d!).valueOf(), expected);
        return;
    }
    if (d != undefined && r != undefined) {
        arktest.assertEQ(new Decimal(n).toDecimalPlaces(d!, r!).valueOf(), expected);
        return;
    }
}

function testToDecimalPlaces001() {
    checkToDecimalPlacesResult('0', 0);
    checkToDecimalPlacesResult('-1', -1);
    checkToDecimalPlacesResult('9.999e+9000000000000000', '9.999e+9000000000000000');
    checkToDecimalPlacesResult('-9.999e+9000000000000000', '-9.999e+9000000000000000');
    checkToDecimalPlacesResult('1e-9000000000000000', '1e-9000000000000000');
    checkToDecimalPlacesResult('-1e-9000000000000000', '-1e-9000000000000000');
    checkToDecimalPlacesResult('Infinity', Infinity);
    checkToDecimalPlacesResult('-Infinity', -Infinity);
    checkToDecimalPlacesResult('NaN', NaN);
    checkToDecimalPlacesResult('Infinity', 'Infinity', 0);
    checkToDecimalPlacesResult('-Infinity', '-Infinity', 0);
}

