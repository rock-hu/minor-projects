package conversion_int.test;

const buffer = new ArrayBuffer(1);
const dataView = new DataView(buffer);
dataView.setUint8(0, 255)

class DeferentIntTypes {
    systemHexadecimal = 16;
    systemBinary = 2;
    int: int = 1;
    number: number = 1;
    bigInt: BigInt = 1n;
    float: float = 1.1;
    intString: string = '1';
    long: long = 1;
    double: double = 1;
    char: char = 1;
    byte = dataView.getUint8(0);
}
