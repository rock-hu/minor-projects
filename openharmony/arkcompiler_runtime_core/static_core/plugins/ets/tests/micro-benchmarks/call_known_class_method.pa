.record A {
    i32 ba
    i32 bar
    i32 baz
}
.record B {
    i32 ba
    i32 bar
    i32 baz
}

.function i32 A.baz(A a0) {
    ldai 2
    return
}

.function void A.baz(A a0, i32 a1) {
    return.void
}

.function void B.bar(B a0, i32 a1) {
    return.void
}

.function void test_1(A a0) {
    call.short A.baz:(A), a0
    movi v0, 2
    add2 v0
    sta v0
    call.short A.baz:(A, i32), a0, v0
    return.void
}

.function void test_2(A a0, B a1) {
    call.short A.baz:(A), a0
    movi v0, 3
    add2 v0
    sta v0
    call.short B.bar, a1, v0
    return.void
}

.function void prolog(A a0) {
    ldai 0
    stobj a0, A.baz
    return.void
}

.function i32 epilog(B a0) {
    ldobj a0, B.bar
    movi v0, 0
    jne v0, error
    ldai 0
    return
error:
    ldai 1
    return
}
