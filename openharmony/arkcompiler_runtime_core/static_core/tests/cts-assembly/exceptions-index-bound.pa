# Check IndexOutOfBoundsException
.record panda.String <external>
.record panda.IndexOutOfBoundsException <external>

.function panda.String panda.IndexOutOfBoundsException.getMessage(panda.IndexOutOfBoundsException a0) <external>

.function i32 main() {
try_begin:
    newobj v0, panda.IndexOutOfBoundsException
    throw v0
    ldai 3
try_end:
    return
catch_block1_begin:
    sta.obj v0
    call.virt panda.IndexOutOfBoundsException.getMessage, v0
    jnez.obj return_error # Exception message is not expected
    ldai 0
    return

catch_block2_begin:
    ldai 1
    return

return_error:
    ldai 2
    return

.catch panda.IndexOutOfBoundsException, try_begin, try_end, catch_block1_begin
.catchall try_begin, try_end, catch_block2_begin
}
