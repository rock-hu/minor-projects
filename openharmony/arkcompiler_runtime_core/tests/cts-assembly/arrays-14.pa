.record panda.String <external>
.record panda.NegativeArraySizeException <external>
.record IO <external>

.function panda.String panda.NegativeArraySizeException.getMessage(panda.NegativeArraySizeException a0) <external>
.function void IO.printString(panda.String a0) <external>

# check NegativeArraySizeException in case of load from array

.function i32 main() {
    movi v0, -1
try_begin:
    newarr v1, v0, i32[]
    ldai 2
    return
try_end:

catch_block1_begin:
    sta.obj v0
    call.virt panda.NegativeArraySizeException.getMessage, v0
    sta.obj v0
    call IO.printString, v0
    ldai 0
    return

catch_block2_begin:
    ldai 1
    return

.catch panda.NegativeArraySizeException, try_begin, try_end, catch_block1_begin
.catchall try_begin, try_end, catch_block2_begin
}
