# Running this test asserts correct usage of runtime option (e.g. heap-size-limit) with value > 2 GB
# Test content copies compiler_effective_types.pa

.function f32 foo(f32 a0) {
    fmovi v0, 1.0
    lda a0
    fcmpg v0
    jnez exit_failure
    fldai 1.0
    return
exit_failure:
    fldai 99.0
    return
}

.function i32 main() {
    fmovi v1, 1.0
    call.short foo, v1, v1
    fcmpg v1
    jnez exit_failure
    ldai 0
    return
exit_failure:
    ldai 1
    return
}
