.record System <external>
.function i64 System.nanoTime() <external>

.function i32 main(){
    call.short System.nanoTime
    sta.64 v0
    movi v1, 0
    movi v2, 100
    ldai 0
loop:
    jeq v2, loop_exit
    inci v1, 1
    lda v1
    jmp loop
loop_exit:
    call.short System.nanoTime
    cmp.64 v0
    jlez exit_failure
    ldai 0
    return
exit_failure:
    ldai 1
    return
}
