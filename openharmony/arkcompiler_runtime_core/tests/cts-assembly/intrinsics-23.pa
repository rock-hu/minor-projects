.record panda.String <external>
.record Convert <external>
.function i32 Convert.stringToI32(panda.String a0) <external>

.function i32 main(){
    # Convert.stringToI32("-1") == -1
    lda.str "-1"
    sta.obj v0
    call Convert.stringToI32, v0
    movi v1, -1
    jne v1, exit_failure

    # Convert.stringToI32("1") == 1
    lda.str "1"
    sta.obj v0
    call Convert.stringToI32, v0
    movi v1, 1
    jne v1, exit_failure

    # Convert.stringToI32("-2147483648") == -2147483648
    lda.str "-2147483648"
    sta.obj v0
    call Convert.stringToI32, v0
    movi v1, -2147483648
    jne v1, exit_failure

    # Convert.stringToI32("-2147483649") == 2147483647
    lda.str "-2147483649"
    sta.obj v0
    call Convert.stringToI32, v0
    movi v1, 2147483647
    jne v1, exit_failure

    # Convert.stringToI32("2147483647") == 2147483647
    lda.str "2147483647"
    sta.obj v0
    call Convert.stringToI32, v0
    movi v1, 2147483647
    jne v1, exit_failure

    # Convert.stringToI32("2147483648") == -2147483648
    lda.str "2147483648"
    sta.obj v0
    call Convert.stringToI32, v0
    movi v1, -2147483648
    jne v1, exit_failure

    ldai 0
    return

exit_failure:
    ldai 1
    return
}
