.record panda.String <external>
.record Convert <external>
.function u32 Convert.stringToU32(panda.String a0) <external>

.function i32 main(){
    # Convert.stringToI32("-1") == -1
    lda.str "-1"
    sta.obj v0
    call Convert.stringToU32, v0
    movi v1, 4294967295
    ucmp v1
    jnez exit_failure

    # Convert.stringToU32("1") == 1
    lda.str "1"
    sta.obj v0
    call Convert.stringToU32, v0
    movi v1, 1
    ucmp v1
    jnez exit_failure

    # Convert.stringToU32("4294967295") == 4294967295
    lda.str "4294967295"
    sta.obj v0
    call Convert.stringToU32, v0
    movi v1, 4294967295
    ucmp v1
    jnez exit_failure

    # Convert.stringToU32("4294967296") == 0
    lda.str "4294967296"
    sta.obj v0
    call Convert.stringToU32, v0
    movi v1, 0
    ucmp v1
    jnez exit_failure

    ldai 0
    return

exit_failure:
    ldai 1
    return
}
