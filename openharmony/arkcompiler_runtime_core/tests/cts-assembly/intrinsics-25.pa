.record panda.String <external>
.record Convert <external>
.function i64 Convert.stringToI64(panda.String a0) <external>

.function i32 main(){
    # Convert.stringToI64("-1") == -1
    lda.str "-1"
    sta.obj v0
    call Convert.stringToI64, v0
    movi.64 v1, -1
    cmp.64 v1
    jnez exit_failure

    # Convert.stringToI64("1") == 1
    lda.str "1"
    sta.obj v0
    call Convert.stringToI64, v0
    movi.64 v1, 1
    cmp.64 v1
    jnez exit_failure

    # Convert.stringToI64("-9223372036854775808") == -9223372036854775808
    lda.str "-9223372036854775808"
    sta.obj v0
    call Convert.stringToI64, v0
    movi.64 v1, -9223372036854775808
    cmp.64 v1
    jnez exit_failure

    # Convert.stringToI64("9223372036854775807") == 9223372036854775807
    lda.str "9223372036854775807"
    sta.obj v0
    call Convert.stringToI64, v0
    movi.64 v1, 9223372036854775807
    cmp.64 v1
    jnez exit_failure

    ldai 0
    return

exit_failure:
    ldai 1
    return
}
