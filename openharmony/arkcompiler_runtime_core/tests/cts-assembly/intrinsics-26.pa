.record panda.String <external>
.record Convert <external>
.function u64 Convert.stringToU64(panda.String a0) <external>

.function i32 main(){
    # Convert.stringToU64("-1") == 18446744073709551615
    lda.str "-1"
    sta.obj v0
    call Convert.stringToU64, v0
    movi.64 v1, -1
    ucmp.64 v1
    jnez exit_failure

    # Convert.stringToU64("1") == 1
    lda.str "1"
    sta.obj v0
    call Convert.stringToU64, v0
    movi.64 v1, 1
    ucmp.64 v1
    jnez exit_failure

    # Convert.stringToU64("18446744073709551615") == 18446744073709551615
    lda.str "18446744073709551615"
    sta.obj v0
    call Convert.stringToU64, v0
    movi.64 v1, -1
    ucmp.64 v1
    jnez exit_failure

    ldai 0
    return

exit_failure:
    ldai 1
    return
}
