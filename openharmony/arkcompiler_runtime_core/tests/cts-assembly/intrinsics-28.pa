.record panda.String <external>
.record Convert <external>
.function f64 Convert.stringToF64(panda.String a0) <external>

.function i32 main(){
    # Convert.stringToF64("-1.0") == -1.0
    lda.str "-1.0"
    sta.obj v0
    call Convert.stringToF64, v0
    fmovi.64 v1, -1.0
    fcmpg.64 v1
    jnez exit_failure

    # Convert.stringToF64("1.0") == 1.0
    lda.str "1.0"
    sta.obj v0
    call Convert.stringToF64, v0
    fmovi.64 v1, 1.0
    fcmpg.64 v1
    jnez exit_failure

    ldai 0
    return

exit_failure:
    ldai 1
    return
}
