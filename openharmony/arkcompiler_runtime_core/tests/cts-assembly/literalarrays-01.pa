#assert that operations with array of i64 works fine
#i64[] array = [0, 1, 2, 3, 4];
#for(i=4; i>=0; i--) assert_eq(i, array[i]);

.array array_i64 i64 5 {0 1 2 3 4}

.function i32 main(){
    lda.const v0, array_i64
    movi v1, 4
    ldai 4
loop:
    jltz loop_exit
    ldarr.64 v0
    i64toi32
    jne v1, exit_failure
    inci v1, -1
    lda v1
    jmp loop
loop_exit:
    ldai 0
    return
exit_failure:
    ldai 1
    return
}
