#Assert that operation ldstatic.obj works fine
.record Asm{
    i32[] asm1 <static>
}
.function i32 main(){
    movi v0, 7
    newarr v1, v0, i32[]
    lda.obj v1
    ststatic.obj Asm.asm1
    ldai 0
    ldstatic.obj Asm.asm1
    sta.obj v1
    lenarr v1
    jne v0, exit_failure
    ldai 0
    return
exit_failure:
    ldai 1
    return
}
