#Assert that operation ucmp works fine
.function i32 main(){
    movi v0, 5
    movi v1, 7
    lda v0
    ucmp v1
    jgez exit_failure
    lda v0
    ucmp v0
    jnez exit_failure
    lda v1
    ucmp v0
    jlez exit_failure
    ldai 0
    return
exit_failure:
    ldai 1
    return
}
