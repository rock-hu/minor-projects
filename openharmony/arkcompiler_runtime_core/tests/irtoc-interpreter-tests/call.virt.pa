.record A {
  i32 a
}

.function void A.init(A a0, i32 a1) <ctor> {
  lda a1
  stobj a0, A.a
  return.void
}

.function i32 A.get_a(A a0) {
  ldobj a0, A.a
  return
}

.function i32 main() {
  movi v0, 42
  initobj.short A.init, v0
  sta.obj v1
  call.virt.short A.get_a, v1
  addi -42
  jnez exit
  lda.obj v1
  call.virt.acc.short A.get_a, v0, 0
  addi -42
exit:
  return
}
