.record A {
  i32 a
}

.function void A.initshort(A a0, i32 a1, i32 a2) <ctor> {
  lda a1
  add2 a2
  stobj a0, A.a
  return.void
}

.function void A.init(A a0, i32 a1, i32 a2, i32 a3) <ctor> {
  lda a2
  add2 a3
  stobj a0, A.a
  return.void
}

.function i32 main() {
  movi v0, 20
  movi v1, 22
  initobj.short A.initshort, v0, v1
  sta.obj v2
  ldobj v2, A.a
  addi -42
  jnez exit
  initobj A.init, v0, v1, v0
  sta.obj v2
  ldobj v2, A.a
  addi -42
  jnez exit
  initobj.range A.initshort, v0
  sta.obj v2
  ldobj v2, A.a
  addi -42
exit:
  return
}
