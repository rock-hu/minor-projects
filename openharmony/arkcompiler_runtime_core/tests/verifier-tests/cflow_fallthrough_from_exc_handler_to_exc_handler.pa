.record E {}

.function u1 error () {
# fallthrough from one handler to another
try_begin:
    ldai 0
try_end:
    return
catch1_begin:
    ldai 1
catch1_end:

catch2_begin:
    return
catch2_end:

.catchall try_begin, try_end, catch1_begin, catch1_end
.catch E, try_begin, try_end, catch2_begin, catch2_end
}

.function u1 good () {
# direct jump from one handler to another
try_begin:
    ldai 0
try_end:
    return
catch1_begin:
    return
catch1_end:

catch2_begin:
    return
catch2_end:

.catchall try_begin, try_end, catch1_begin, catch1_end
.catch E, try_begin, try_end, catch2_begin, catch2_end
}

.function i32 main() <static> {
    ldai 0
    return
}
