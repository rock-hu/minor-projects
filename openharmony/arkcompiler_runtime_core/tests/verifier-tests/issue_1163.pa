.function i32 main() {
#
    movi.64 v0, 0x777777777FFFFFFF
    movi.64 v1, 0x1111111180000000
    add v0, v1
    movi.64 v0, 0xFFFFFFFFFFFFFFFF
    cmp.64 v0
#   check positive
    movi v0, 0 # expected result
    jne v0, exit_failure
    ldai 0 # passed
    return
exit_failure:
    ldai 1 # failed
    return
}
#
.function u1 main_exitcode_wrapper() {
    call main
    jeqz wrapper_exit_positive
    ldai 81
    return
wrapper_exit_positive:
    ldai 80
    return
}
