<stack id="stack"
       style="left: 0px; top: 0px; width: 454px; height: 454px;">
  <list id="list"
        style="left: 0px; top: 0px; width: 454px; height: 454px; flex-direction: column">
    <list-item id="listItem0"
               style="width: 454px; height: 60px;">
    </list-item>
    <list-item id="listItem1"
               style="width: 454px; height: 80px;">
      <input id="buttonBack"
             type="button"
             style="margin-left: 85px; width: 130px; height: 60px;"
             value="返回首页"
             onclick="backMain">
      </input>
      <input id="buttonPrev"
             type="button"
             style="margin-left: 20px; width: 130px; height: 60px;"
             value="返回上级"
             onclick="changePrev">
      </input>
    </list-item>
    <list-item id="listItem2"
               class="separatorContainerStyle">
    </list-item>
    <list-item id="listItem3"
               class="titleContainerStyle">
      <text id="text"
            class="titleTextStyle">
        canvas绘图接口
      </text>
    </list-item>
    <list-item id="listItem4"
               class="separatorContainerStyle">
    </list-item>
    <list-item id="listItem5"
               style="width: 454px; height: 170px;">
      <canvas id="canvas1"
              ref="canvas1"
              style="margin-left: 45px; width: 200px; height: 150px; background-color: #ffff00">
      </canvas>
      <input id="button1"
             type="button"
             style="margin-left: 10px; width: 180px; height: 100px;"
             value="lineTo_Test001"
             onclick="lineTo001Func">
      </input>
    </list-item>
    <list-item id="listItem6"
               class="separatorContainerStyle">
    </list-item>
    <list-item id="listItem7"
               style="width: 454px; height: 170px;">
      <canvas id="canvas2"
              ref="canvas2"
              style="margin-left: 45px; width: 200px; height: 150px; background-color: #ffff00">
      </canvas>
      <input id="button2"
             type="button"
             style="margin-left: 10px; width: 180px; height: 100px;"
             value="lineTo_Test002"
             onclick="lineTo002Func">
      </input>
    </list-item>
    <list-item id="listItem8"
               class="separatorContainerStyle">
    </list-item>
    <list-item id="listItem9"
               style="width: 454px; height: 170px;">
      <canvas id="canvas3"
              ref="canvas3"
              style="margin-left: 45px; width: 200px; height: 150px; background-color: #ffff00">
      </canvas>
      <input id="button3"
             type="button"
             style="margin-left: 10px; width: 180px; height: 100px;"
             value="lineTo_Test003"
             onclick="lineTo003Func">
      </input>
    </list-item>
  </list>
</stack>