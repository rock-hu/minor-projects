<div id="containerDiv"
     class="container"
     style="flex-direction: column; align-items: center">
  <div id="frameDiv0"
       style="margin-top: 40px; width: 380px; height: 80px;"
       class="divFrame">
    <text id="frameText"
          style="width: 380px; height: 80px; text-overflow: ellipsis">
      Component_chart_AttributeSet_Test_029
    </text>
  </div>
  <div id="frameDiv1"
       style="height: 220px; flex-direction: column;"
       class="divFrame">
    <div id="chartDiv1"
         style="width: 454px; height: 50px">
      <text id="chartText1"
            style="width: 250px; height: 50px">
        head size:-1
      </text>
      <chart id="chart1"
             style="width: 200px; height: 50px"
             type="line"
             style="left: 0px; top: 0px;"
             ref="chart1"
             datasets="{{datasets}}"
             options="{{options1}}">
      </chart>
    </div>
    <div id="chartDiv2"
         style="width: 454px; height: 50px">
      <text id="chartText2"
            style="width: 250px; height: 50px">
        bottom shape: aaa size:3 strWid:5
      </text>
      <chart id="chart2"
             style="width: 200px; height: 50px"
             type="line"
             style="left: 0px; top: 0px;"
             ref="chart2"
             datasets="{{datasets}}"
             options="{{options2}}">
      </chart>
    </div>
    <div id="chartDiv3"
         style="width: 454px; height: 100px;">
      <text id="chartText3"
            style="width: 250px; height: 100px; text-overflow: ellipsis">
        top strWid:5 strCol:0x0000ff fillCol:0x00ff00
      </text>
      <chart id="chart3"
             style="width: 200px; height: 50px"
             type="line"
             style="left: 0px; top: 0px;"
             ref="chart3"
             datasets="{{datasets}}"
             options="{{options3}}">
      </chart>
    </div>
  </div>
  <div id="frameDiv2"
       class="divFrame">
    <input id="buttonPrev"
           type="button"
           value="上页"
           style="width: 90px; height: 40px;"
           onclick="changePrev" />
    <input id="buttonBack"
           type="button"
           value="首页"
           style="width: 90px; height: 40px;"
           onclick="backMain" />
    <input id="buttonNext"
           type="button"
           value="下页"
           style="width: 90px; height: 40px;"
           onclick="changeNext" />
  </div>
</div>