<div id="containerDiv"
     class="container"
     style="flex-direction: column; align-items: center">
  <div id="frameDiv0"
       style="margin-top: 40px; width: 320px; height: 80px;"
       class="divFrame">
    <text id="frameText"
          style="width: 320px; height: 80px; text-overflow: ellipsis">
      Component_Div_StyleSet_Test_036
    </text>
  </div>
  <div id="frameDiv1"
       style="height: 270px; flex-direction: column; justify-content: flex-end;"
       class="divFrame">
    <div id="itemDiv0"
         style="width: {{divWidth}}px; height: {{divHeight}}px; border-width: {{divBorder}}px;
                border-color: red;
         padding: {{divPadding}} px; background-color: green; flex-wrap: wrap;"
         onclick="changeFocusToDiv">
      <input id="button"
             style="width: {{buttonWidth}}px; height: {{buttonHeight}}px; border-color: yellow;
             border-width: {{buttonBorder}}px; padding: {{buttonPadding}}px"
             type="button"
             onclick="changeFocusToButton"
             value="size include border and padding test">
      </input>
      <text id="text"
            style="width: 350px">
        click button above or div to change focus
      </text>
    </div>
    <div id="itemDiv1"
         style="flex-wrap: wrap; width: 454px; height: 80px;">
      <input id="increaseWidth"
             style="width: 110px; height: 40px;"
             value="+width"
             onclick="increaseWidth">
      </input>
      <input id="increaseHeight"
             style="width: 120px; height: 40px;"
             value="+height"
             onclick="increaseHeight">
      </input>
      <input id="increasePadding"
             style="width: 90px; height: 40px;"
             value="+pad"
             onclick="increasePadding">
      </input>
      <input id="increaseBorder"
             style="width: 130px; height: 40px;"
             value="+border"
             onclick="increaseBorder">
      </input>
      <input id="reduceWidth"
             style="width: 110px; height: 40px;"
             value="-width"
             onclick="reduceWidth">
      </input>
      <input id="reduceHight"
             style="width: 120px; height: 40px;"
             value="-height"
             onclick="reduceHight">
      </input>
      <input id="reducePadding"
             style="width: 90px; height: 40px;"
             value="-pad"
             onclick="reducePadding">
      </input>
      <input id="reduceBorder"
             style="width: 130px; height: 40px;"
             value="-border"
             onclick="reduceBorder">
      </input>
    </div>
  </div>
  <div id="frameDiv2"
       class="divFrame">
    <input id="buttonPrev"
           type="button"
           value="上页"
           style="width: 90px; height: 40px;"
           onclick="changePrev" />
    <input id="buttonBack"
           type="button"
           value="首页"
           style="width: 90px; height: 40px;"
           onclick="backMain" />
  </div>
</div>